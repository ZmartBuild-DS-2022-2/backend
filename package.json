{
  "name": "backend",
  "version": "1.0.0",
  "description": "A simple API Rest for backend",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "format": "prettier --check .",
    "format-write": "prettier --write .",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "prepare": "husky install",
    "db-migrate": "sequelize db:migrate",
