{
  "name": "backend",
  "version": "1.0.0",
  "description": "A simple API Rest for backend",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "format": "prettier --check .",
    "format-write": "prettier --write .",
    "pf": "prettier --write .",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "prepare": "husky install",
    "db-migrate": "sequelize db:migrate",
    "db-migrate-undo": "sequelize db:migrate:undo",
    "db-seed": "sequelize db:seed:all",
    "db-seed-undo": "sequelize db:seed:undo",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --silent"
  },
  "author": "FeBalla",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1250.0",
    "babel": "^6.23.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "eslint-plugin-jest": "^27.1.5",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "node-mime-types": "^1.1.0",
    "pg": "^8.8.0",
    "sequelize": "^6.21.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "babel-jest": "^29.3.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "sequelize-cli": "^6.5.1",
    "supertest": "^6.3.1"
  }
}
